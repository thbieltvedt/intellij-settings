<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="1" />
    <option name="COMPLETION_CASE_SENSITIVE" value="1" />
    <option name="HIGHLIGHT_IDENTIFIER_UNDER_CARET" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="3" />
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="19" />
    <option name="FONT_FAMILY" value="Menlo" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.3" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.*;*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="kotlin.KotlinLambdasHintsProvider" />
        <option value="kotlin.microservices.url.path.inlay.hints" />
      </set>
    </option>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
  </component>
</application>